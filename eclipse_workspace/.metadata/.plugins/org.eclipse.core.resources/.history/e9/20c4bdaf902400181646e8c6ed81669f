package ronan_hanley.galway_game_jam.nine.entity;

import org.newdawn.slick.Animation;
import org.newdawn.slick.Image;
import org.newdawn.slick.SlickException;

public class Human extends MovingEntity {
	private static Animation anim;
	
	static {
		anim = new Animation();
		try {
			anim.addFrame(new Image("res/human.png"), 1);
		} catch (SlickException e) {
			e.printStackTrace();
		}
	}
	
	public Human(int initialX, int initialY, double maxSpeed, int sightDistance) {
		super(initialX, initialY, maxSpeed, anim);
	}
	
}
