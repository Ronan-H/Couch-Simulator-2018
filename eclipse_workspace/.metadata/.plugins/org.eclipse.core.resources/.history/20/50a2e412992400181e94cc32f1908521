package ronan_hanley.galway_game_jam.nine.entity;

public class BackAndForthMovementPattern extends MovementPattern {
	private int destX, destY;
	private boolean upPhase;
	
	public BackAndForthMovementPattern(Human human, int destX, int destY) {
		super(human);
		this.destX = destX;
		this.destY = destY;
		
		upPhase = true;
	}

	@Override
	public void tick() {
		human.goMaxSpeed();
		getHuman().goUp();
	}
	
}
