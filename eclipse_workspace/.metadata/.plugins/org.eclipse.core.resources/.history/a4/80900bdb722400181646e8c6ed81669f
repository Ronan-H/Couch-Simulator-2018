public class Level {
	private Image image;
	private boolean[] pixels;
	
	public Level(int levelNum) {
		try {
			image = new Image(String.format("res/level%d.png", levelNum));
		} catch (SlickException e) {
			e.printStackTrace();
		}
	}
	
	public boolean collides(MovingEntity e) {
		
	}
	
}
